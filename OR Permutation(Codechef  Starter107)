#include <bits/stdc++.h>
using namespace std;

void permutation(vector<long long> &stor, int N) {
    for (int i = N; i > 0; i--) {
        stor.push_back(i);
    }
    for (int i = 0; i < N; i++) {
        cout << stor[i] << " ";
    }
    cout << endl;
}

int main() {
    long long int t;
    cin >> t;
    for (int i = 1; i <= t; i++) {
        long long int N;
        cin >> N;
        vector<long long> stor;
        permutation(stor, N);
    }
    return 0;
}
